{
    "manifest_version": 3,
    "name": "My tab extension",
    "short_name": "MySmartTab",
    "description": "New tab manager Extension",
    "version": "1.0",
    "author": "Chaitanya Lingayat",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "scripting",
        "storage"
    ],
    "icons": {
        "16": "favicon-16x16.png",
        "32": "favicon-32x32.png",
        "48": "favicon-48x48.png",
        "128": "favicon-128x128.png",
        "192": "favicon-192x192.png",
        "512": "favicon-512x512.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "favicon-16x16.png",
            "32": "favicon-32x32.png",
            "48": "favicon-48x48.png",
            "128": "favicon-128x128.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://*/*",
                "http://*/*"
            ],
            "all_frames": true,
            "css": [
                "index.css"
            ],
            "run_at": "document_start"
        }
    ],
    "theme_color": "#2563eb",
    "background_color": "#ffffff",
    "display": "standalone",
    "start_url": "/"
}