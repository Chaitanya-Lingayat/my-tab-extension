{
    "manifest_version": 3,
    "name": "My tab extension",
    "description": "New tab manager Extension",
    "version": "1.0",
    "author": "Chaitanya Lingayat",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "scripting",
        "storage"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.svg"
    },
    "content_scripts": [
        {
            "matches": [
                "https://*/*",
                "http://*/*"
            ],
            "all_frames": true,
            "css": [
                "index.css"
            ],
            "run_at": "document_start"
        }
    ]
}